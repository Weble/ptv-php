{
    "name": "weble/ptv-php",
    "description": "PTV Api PHP SDK",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "PTV\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Daniele Rosario",
            "email": "daniele@weble.it"
        }
    ],
    "require": {
        "saloonphp/saloon": "^3.0",
        "moneyphp/money": "^4.5"
    },
    "require-dev": {
        "pestphp/pest": "^2.34",
        "vlucas/phpdotenv": "^5.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
